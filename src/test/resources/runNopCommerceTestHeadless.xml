<?xml version="1.0" encoding="UTF-8"?>
<!DOCTYPE suite SYSTEM "http://testng.org/testng-1.0.dtd" >
<suite parallel="false" name="NopCommerce">



  <test name="Run on Chrome Headless">	
	<parameter name="browser" value="h_chrome"></parameter>
    <classes>
      <class name="com.nopcommerce.user.Level03_Login_Apply_BasePage_PageObject_MultiBrowser"/>
    </classes>
  </test> 

  <test name="Run on Firefox Headless">	
	<parameter name="browser" value="h_firefox"></parameter>
    <classes>
      <class name="com.nopcommerce.user.Level03_Login_Apply_BasePage_PageObject_MultiBrowser"/>
    </classes>
  </test> 

 <test name="Run on Opera Headless">	
	<parameter name="browser" value="opera"></parameter>
    <classes>
      <class name="com.nopcommerce.user.Level03_Login_Apply_BasePage_PageObject_MultiBrowser"/>
    </classes>
  </test> 

 

</suite> 